{
  "AppSettings" : {
    "Secret" : "$Secret",
    "Emailfrom": "$Emailfrom",
    "SmtpHost": "$SmtpHost",
    "SmtpPort" : "$SmtpPort",
    "SmtpUser" : "$SmtpUser",
    "SmtpPass" : "$SmtpPass",
    "DisplayedSender" : "$DisplayedSender"
  },
  "ConnectionStrings": {
    "LHMS" : "$ConnectString"
},
    "Logging": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "AllowedHosts": "*"
}