{
  "AppSettings" : {
      "Secret" : "$Secret",
      "Emailfrom": "$Emailfrom",
      "SmtpHost": "$SmtpHost",
      "SmtpPort" : "$SmtpPort",
      "SmtpUser" : "$SmtpUser",
      "SmtpPass" : "$SmtpPass",
      "DisplayedSender" : "$DisplayedSender"
    },
    "ConnectionStrings": {
      "SystemReports" : "$ConnectString"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
